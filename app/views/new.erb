<h1>Create a New Robot</h1>
<form action="/robots" method="post">
  <input type ='text' name="robot[name]" placeholder="robot name"/><br/>
  <input type ='text' name="robot[city]" placeholder="city"/><br/>
  <input type ='text' name="robot[state]" placeholder="state"/><br/>
  <input type ='text' name="robot[avatar]" placeholder="avatar"/><br/>
  <input type ='text' name="robot[birthdate]" placeholder="birthdate: MM-DD-YYYY"/><br/>
  <input type ='text' name="robot[date_hired]" placeholder="date hired: MM-DD-YYYY"/><br/>
  <input type ='text' name="robot[department]" placeholder="department"/><br/>
  <input class ='btn btn-default btn-warning' type = "submit" value="Create Robot"/>
</form>
